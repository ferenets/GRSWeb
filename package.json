  "scripts": {
    "babel-node": "babel-node --presets babel-preset-es2015",
    "server": "nodemon --watch ./src/server/ --exec \"npm run babel-node\" ./src/server/boot/app/app.js | bunyan",
    "client": "nodemon --watch ./webpack-client.config.dev.js --watch ./src/client-dev-server/ ./src/client-dev-server/client-dev-server.js",
    "build-client": "webpack -p --progress --config webpack-client.config.build.js",
    "build-server-compact": "webpack --progress --config webpack-server.config.build.js",
    "build-server": "babel --presets babel-preset-es2015 ./src/server --out-dir ./build/server",
    "build": "gulp clean && npm run build-client && npm run build-server && gulp copy-assets",
    "postinstall": "npm run first-replacement && npm run second-replacement && npm run third-replacement && npm run fourth-replacement && npm run fifth-replacement && npm run sixth-replacement && npm run seventh-replacement && npm run eighth-replacement && npm run ninth-replacement && npm run tenth-replacement  && npm run eleventh-replacement && npm run twelfth-replacement && npm run thirteenth-replacement",
    "first-replacement": "gsed -i '785 s/timeFormat/timeFormatUaUa/' node_modules/react-d3-core/node_modules/d3-scale/build/d3-scale.js",
    "second-replacement": "gsed -i '61 s/formatHour12/formatHour24/' node_modules/react-d3-core/node_modules/d3-time-format/build/d3-time-format.js",
    "third-replacement": "gsed -i '777i var uaUa = locale$1({\\\"decimal\\\": \\\".\\\",\"thousands\": \",\",\"currency\": [\"грн\", \"\"],\"periods\": [\"\"],\"days\": [\"Неділя\", \"Понеділок\", \"Вівторок\", \"Середа\", \"Четвер\", \"П\\x27ятниця\", \"Субота\"],\"shortDays\": [\"Нд\", \"Пн\", \"Вт\", \"Ср\", \"Чт\", \"Пт\", \"Сб\"],\"months\": [\"січня\", \"лютого\", \"березня\", \"квітня\", \"травня\", \"червня\", \"липня\", \"серпня\", \"вересня\", \"жовтня\", \"листопада\", \"грудня\"],\"shortMonths\": [\"січ\", \"лют\", \"бер\", \"кві\", \"тра\", \"чер\", \"лип\", \"сер\", \"вер\", \"жов\", \"лис\", \"гру\"]});' node_modules/react-d3-core/node_modules/d3-time-format/build/d3-time-format.js",
    "fourth-replacement": "gsed -i '832i exports.timeFormatUaUa = uaUa.format;' node_modules/react-d3-core/node_modules/d3-time-format/build/d3-time-format.js",
    "fifth-replacement": "gsed -i '681 s/\".%L\"/\"%d.%m.%y %H:%M\"/' node_modules/react-d3-core/node_modules/d3-scale/build/d3-scale.js",
    "sixth-replacement": "gsed -i '682 s/\":%S\"/\"%d.%m.%y %H:%M\"/' node_modules/react-d3-core/node_modules/d3-scale/build/d3-scale.js",
    "seventh-replacement": "gsed -i '683 s/\"%I:%M\"/\"%d.%m.%y %H:%M\"/' node_modules/react-d3-core/node_modules/d3-scale/build/d3-scale.js",
    "eighth-replacement": "gsed -i '684 s/\"%I %p\"/\"%d.%m.%y %H:%M\"/' node_modules/react-d3-core/node_modules/d3-scale/build/d3-scale.js",
    "ninth-replacement": "gsed -i '685 s/\"%a %d\"/\"%d.%m.%y %H:%M\"/' node_modules/react-d3-core/node_modules/d3-scale/build/d3-scale.js",
    "tenth-replacement": "gsed -i '686 s/\"%b %d\"/\"%d.%m.%y %H:%M\"/' node_modules/react-d3-core/node_modules/d3-scale/build/d3-scale.js",
    "eleventh-replacement": "gsed -i '687 s/\"%B\"/\"%d.%m.%y %H:%M\"/' node_modules/react-d3-core/node_modules/d3-scale/build/d3-scale.js",
    "twelfth-replacement": "gsed -i '688 s/\"%Y\"/\"%d.%m.%y %H:%M\"/' node_modules/react-d3-core/node_modules/d3-scale/build/d3-scale.js",
    "thirteenth-replacement": "gsed -i '56i title = new Date(title).toLocaleString(\"ru\", {day: \"2-digit\", month: \"2-digit\", year: \"numeric\", hour: \"numeric\", minute:\"2-digit\", hour12: false});' node_modules/react-d3-tooltip/lib/tooltip/simple/index.js",
    "cs": "node ./node_modules/eslint/bin/eslint.js src/**/*.js",
    "cs:fix": "node ./node_modules/eslint/bin/eslint.js --fix src/**/*.js",
    "test": "npm run cs"
  },
